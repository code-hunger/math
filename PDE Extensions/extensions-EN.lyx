\begin_layout Proof
\begin_inset Formula 
\[
F_{\sigma}(x)=f\left(\frac{x}{\sigma}\right),
\]

\end_inset

 and after that we'll average the expanded function in the narrower domain
 
\begin_inset Formula $K_{a}\subset K_{\sigma a}$
\end_inset

.
\begin_inset FormulaMacro
\newcommand{\red}[1]{{\color{red}#1}}
\end_inset


\begin_inset FormulaMacro
\newcommand{\blue}[1]{{\color{blue}#1}}
\end_inset


\end_layout

\begin_deeper
\begin_layout Paragraph
Bound on the expansion 
\begin_inset Formula $\left\Vert f-F_{\sigma}\right\Vert _{H^{k}(K_{a})}$
\end_inset


\end_layout

\end_deeper
\begin_layout Proof
To evaluate the 
\begin_inset Formula $H^{k}$
\end_inset

-norm, we first evaluate the 
\begin_inset Formula $L_{2}$
\end_inset

-norms of each derivative separately.
 We have
\begin_inset Formula 
\begin{align*}
\left\Vert \da F_{\sigma}(x)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})} & =\left\Vert \da F_{\sigma}(x)\blue{-\varphi_{a}\left(\frac{x}{\sigma}\right)+\varphi_{a}\left(\frac{x}{\sigma}\right)}-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}\\
 & \overset{\triangle\text{-inequality}}{\le}\left\Vert \da F_{\sigma}(x)-\varphi_{a}\left(\frac{x}{\sigma}\right)\right\Vert _{L_{2}(K_{a})}+\left\Vert \varphi_{a}\left(\frac{x}{\sigma}\right)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}\\
 & \le\left\Vert \da F_{\sigma}(x)-\varphi_{a}\left(\frac{x}{\sigma}\right)\right\Vert _{L_{2}{\color{red}(K_{\sigma a})}}+\left\Vert \varphi_{a}\left(\frac{x}{\sigma}\right)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}.
\end{align*}

\end_inset

For the right summand we have 
\begin_inset Formula 
\begin{align*}
\left\Vert \da F_{\sigma}(x)-\varphi_{a}\left(\frac{x}{\sigma}\right)\right\Vert _{L_{2}(K_{\sigma a})} & \overset{\text{def. of }F_{\sigma}}{=}\left\Vert {\color{red}\frac{1}{\sigma^{\left|\alpha\right|}}\da f\left(\frac{x}{\sigma}\right)}-\varphi_{a}\left(\frac{x}{\sigma}\right)\right\Vert _{L_{2}(K_{\sigma a})}\\
 & \overset{\triangle\text{-inequality}}{\le}\left\Vert \left(\frac{1}{\sigma^{\left|\alpha\right|}}{\color{red}-1}\right)\da f\left(\frac{x}{\sigma}\right)\right\Vert _{L_{2}(K_{\sigma a})}+\left\Vert {\color{red}+1}\da f\left(\frac{x}{\sigma}\right)-\varphi_{a}\left(\frac{x}{\sigma}\right)\right\Vert _{L_{2}(K_{\sigma a})}\\
 & =\red{\sigma^{n/2}}\left(1\red -\frac{1}{\sigma^{\left|\alpha\right|}}\right)\left\Vert \da f\left(\red x\right)\right\Vert _{L_{2}(\red{K_{a}})}+\red{\sigma^{n/2}}\left\Vert \da f\left(\red x\right)-\varphi_{a}\left(\red x\right)\right\Vert _{L_{2}(\red{K_{a}})}\\
 & \le\sigma^{n/2}\left(1-\frac{1}{\sigma^{\left|\alpha\right|}}\right)\left\Vert \da f\right\Vert _{L_{2}(K_{a})}+\sigma^{n/2}\red{\varepsilon}
\end{align*}

\end_inset

and substituting above, we get 
\begin_inset Formula 
\begin{align*}
\left\Vert \da F_{\sigma}(x)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}\le\sigma^{n/2} & \left(1-\frac{1}{\sigma^{\left|\alpha\right|}}\right)\left\Vert \da f\right\Vert _{L_{2}(K_{a})}+\sigma^{n/2}\varepsilon\\
 & +\left\Vert \varphi_{a}\left(\frac{x}{\sigma}\right)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}.
\end{align*}

\end_inset

Now 
\begin_inset Formula 
\begin{align*}
\left\Vert \da f-\da F_{\sigma}\right\Vert _{L_{2}(K_{a})} & \le\underbrace{\left\Vert \da f-\varphi_{\alpha}\right\Vert _{L_{2}(K_{a})}}_{\le\varepsilon}+\left\Vert \da F_{\sigma}-\varphi_{\alpha}\right\Vert _{L_{2}(K_{a})}\\
 & \le\left(\red{1+}\sigma^{n/2}\right)\varepsilon+\sigma^{n/2}\left(1-\frac{1}{\sigma^{\left|\alpha\right|}}\right)\left\Vert \da f\right\Vert _{L_{2}(K_{a})}+\left\Vert \varphi_{a}\left(\frac{x}{\sigma}\right)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}.
\end{align*}

\end_inset


\end_layout

\begin_layout Proof
It remains to evaluate the second and the third summand
\end_layout

\begin_deeper
\begin_layout Itemize
For the second summand we have
\begin_inset Formula 
\[
\sigma^{n/2}\left(1-\frac{1}{\sigma^{\left|\alpha\right|}}\right)\xrightarrow[\sigma\to1^{+}]{}0,
\]

\end_inset

therefore for some 
\begin_inset Formula $\sigma>1$
\end_inset

, the second summand is smaller than 
\begin_inset Formula $\varepsilon$
\end_inset

.
\end_layout

\begin_layout Itemize
Since 
\begin_inset Formula $\varphi_{a}$
\end_inset

 is uniformly continuous, it follows 
\begin_inset Formula $\varphi_{\alpha}\left(\frac{x}{\sigma}\right)\xrightarrow[\sigma\to1]{}\varphi_{\alpha}(x)$
\end_inset

, hence for some 
\begin_inset Formula $\sigma>1$
\end_inset

, 
\begin_inset Formula 
\[
\left\Vert \varphi_{a}\left(\frac{x}{\sigma}\right)-\varphi_{\alpha}(x)\right\Vert _{L_{2}(K_{a})}\le\varepsilon.
\]

\end_inset


\end_layout

\begin_layout Standard
From the two bounds it follows that for some 
\begin_inset Formula $\sigma_{0}>1$
\end_inset

 (WLOG 
\begin_inset Formula $\sigma_{0}<2$
\end_inset

 (?? TODO check typo??) )
\begin_inset Formula 
\begin{align*}
\left\Vert \da f-\da F_{\sigma_{0}}\right\Vert _{L_{2}(K_{a})} & \le\left(1+\sigma_{0}^{n/2}\right)\varepsilon+\varepsilon+\varepsilon\\
 & \le4\varepsilon.
\end{align*}

\end_inset

Summing over all 
\begin_inset Formula $\alpha$
\end_inset

 with 
\begin_inset Formula $\left|\alpha\right|\le k$
\end_inset

 we get an analoguous bound of for the 
\begin_inset Formula $H^{k}$
\end_inset

-norm:
\begin_inset Formula 
\[
\left\Vert f-F_{\sigma_{0}}\right\Vert _{H^{k}(K_{a})}\le4n^{k}\varepsilon.
\]

\end_inset


\end_layout

\begin_layout Paragraph
The Averaging
\end_layout

\begin_layout Standard
For sufficiently small 
\begin_inset Formula $h>0$
\end_inset

 (
\begin_inset Formula $h<a(\sigma-1)$
\end_inset

) we look at the averaging 
\begin_inset Formula $\left(F_{\sigma_{0}}\right)_{h}\in C^{\infty}(K_{a})$
\end_inset

 of 
\begin_inset Formula $F_{\sigma_{0}}$
\end_inset

.
 In the narrower domain 
\begin_inset Formula $K_{a}$
\end_inset

 we have 
\begin_inset Formula 
\[
\left\Vert \left(F_{\sigma_{0}}\right)_{h}-F_{\sigma_{0}}\right\Vert _{H^{k}(K_{a})}\xrightarrow[h\to0]{}0,
\]

\end_inset

therefore for sufficiently small 
\begin_inset Formula $h=h_{0}$
\end_inset

, 
\begin_inset Formula 
\[
\left\Vert \left(F_{\sigma_{0}}\right)_{h_{0}}-F_{\sigma_{0}}\right\Vert \le\varepsilon.
\]

\end_inset

It remains to observe that 
\begin_inset Formula 
\begin{align*}
\left\Vert \left(F_{\sigma_{0}}\right)_{h_{0}}-f\right\Vert _{H^{k}(K_{a})} & \le\underbrace{\left\Vert \left(F_{\sigma_{0}}\right)_{h_{0}}-F_{\sigma_{0}}\right\Vert _{H^{k}(K_{a})}}_{\le\varepsilon}+\underbrace{\left\Vert f-F_{\sigma_{0}}\right\Vert }_{\le4n^{k}\varepsilon}\\
 & \le\left(1+4n^{k}\right)\varepsilon,
\end{align*}

\end_inset

which means we can make the averaging close enough to 
\begin_inset Formula $f$
\end_inset

.
\end_layout

\end_deeper
\begin_layout Section
Change of variables in 
\begin_inset Formula $H^{k}$
\end_inset


\end_layout

\begin_layout Lemma*
Let 
\begin_inset Formula $\bar{Q}'\subset Q$
\end_inset

, 
\begin_inset Formula $f\in H^{k}(Q)$
\end_inset

 and 
\begin_inset Formula $\left|\alpha\right|\le k$
\end_inset

.
 Then for 
\begin_inset Formula $h\le\text{dist}\left(Q',\partial Q\right)$
\end_inset

, the derivative of the averaging 
\begin_inset Formula $f_{h}\in C^{\infty}$
\end_inset

 is the averaging of the derivative, that is 
\begin_inset Formula 
\[
\partial^{\alpha}f_{h}=\left(\partial^{\alpha}f\right)_{h}.
\]

\end_inset


\end_layout

\begin_layout Proof
We'll check it for 
\begin_inset Formula $k=1$
\end_inset

.
 Let 
\begin_inset Formula $1\le i\le n$
\end_inset

.
 We have to prove 
\begin_inset Formula 
\[
\frac{\partial}{\partial x_{i}}\int\limits _{Q}\omega_{x,h}(y)\cdot f(y)\dy=\int\limits _{Q}\omega_{x,h}(y)\cdot\frac{\partial f}{\partial y_{i}}(y)\dy.
\]

\end_inset

We write out the definition of the generalized derivative 
\begin_inset Formula $\partial^{\alpha}f$
\end_inset

 in the RHS with a test function 
\begin_inset Formula $\omega_{x,\varepsilon}$
\end_inset

:
\begin_inset Formula 
\[
\int\limits _{Q}\omega_{x,h}(y)\cdot\frac{\partial f}{\partial y_{i}}(y)\dy=-\int\limits _{Q}\frac{\partial\omega_{x,h}}{\partial y_{i}}(y)\cdot f(y)\dy.
\]

\end_inset


\end_layout

\begin_layout Proof
Therefore it remains to convince us that
\begin_inset Formula 
\[
\frac{\partial}{\partial x_{i}}\int\limits _{Q}\omega_{x,h}(y)\cdot f(y)\dy=-\int\limits _{Q}\frac{\partial\omega_{x,h}}{\partial y_{i}}(y)\cdot f(y)\dy.
\]

\end_inset


\end_layout

\begin_layout Proof
We write out the derivative 
\begin_inset Formula 
\[
\frac{\partial\omega_{x,h}(y)}{\partial y_{i}}=\frac{\partial\omega\left(\frac{x-y}{\varepsilon}\right)}{\partial y_{i}}=\frac{1}{\varepsilon}\omega'\left(\frac{x-y}{\varepsilon}\right)=\omega\left(\frac{x-y}{\varepsilon}\right)\frac{1}{\left(1-\left|x\right|^{2}\right)^{2}}\left(2\left|x\right|\right)
\]

\end_inset


\end_layout

\begin_layout Proposition*
Let 
\begin_inset Formula $y:\bar{Q}\to\bar{\Omega}$
\end_inset

 and 
\begin_inset Formula $x:\bar{\Omega}\to\bar{Q}$
\end_inset

 be inverse to each other functions in 
\begin_inset Formula $C^{k}$
\end_inset

.
 
\end_layout

\begin_layout Proposition*
Then, 
\begin_inset Formula $f\in H^{k}(\Omega)$
\end_inset

 if and only if 
\begin_inset Formula $F=f\circ y\in H^{k}(Q)$
\end_inset

, and in that case the derivatives of 
\begin_inset Formula $F$
\end_inset

 are calculated by the usual chain rule.
\end_layout

\begin_layout Proposition*
Moreover, for these 
\begin_inset Formula $x\text{ and }y$
\end_inset

 there exist such constants 
\begin_inset Formula $C_{1},C_{2}$
\end_inset

 that 
\begin_inset Formula 
\[
C_{1}\left\Vert f\right\Vert _{H^{k}(\Omega)}\le\left\Vert F\right\Vert _{H^{k}(Q)}\le C_{2}\left\Vert f\right\Vert _{H^{k}(\Omega)}\quad\text{for all }f\in H^{k}(\Omega).
\]

\end_inset


\end_layout

\begin_layout Proof
Let 
\begin_inset Formula $f\in H^{k}$
\end_inset

.
 First, it is clear that 
\begin_inset Formula $F\in L_{2}$
\end_inset

, because 
\begin_inset Formula $F=f\circ y$
\end_inset

 has an integrable square by the change of variables formula in the Lebesgue
 integral 
\begin_inset Formula $\int_{Q}f(y(x))\text{dx}$
\end_inset

.
\end_layout

\begin_deeper
\begin_layout Paragraph
Existence of the first derivatives
\end_layout

\end_deeper
\begin_layout Proof
As in the previous lemma, we will approximate (the almost everywhere defined)
 
\begin_inset Formula $f\in H^{k}$
\end_inset

 with an averaging in 
\begin_inset Formula $C^{\infty}$
\end_inset

, and then we will see that the derivatives of the average approximate the
 derivatives of 
\begin_inset Formula $f$
\end_inset

.
 
\end_layout

\begin_layout Proof
To perform averaging at all, we need it to be defined "a little outside,"
 so we narrow the considered region to 
\begin_inset Formula $\bar{\Omega}'\subset\Omega$
\end_inset

.
 We set 
\begin_inset Formula $Q'=x(\Omega)$
\end_inset

, and let 
\begin_inset Formula $f_{h}\in C^{\infty}\left(\Omega'\right)$
\end_inset

 be the average of 
\begin_inset Formula $f$
\end_inset

 in 
\begin_inset Formula $\Omega'$
\end_inset

 with a sufficiently small radius 
\begin_inset Formula $h\le\text{dist}\left(\Omega',\partial\Omega\right)$
\end_inset

.
 Then, from the property 
\begin_inset Formula 
\[
\left\Vert f_{h}-f\right\Vert _{L_{2}(Q)}\xrightarrow[h\to0]{}0
\]

\end_inset


\end_layout

\begin_layout Proof
we get (again by change of variables in the integral)
\begin_inset Formula 
\[
\left\Vert F_{h}-F\right\Vert _{L_{2}(Q')}=\left\Vert f_{h}\circ y-f\circ y\right\Vert =\left\Vert \left(f_{h}-f\right)\circ y\right\Vert \xrightarrow[h\to0]{}0.
\]

\end_inset


\end_layout

\begin_layout Proof
We need an analoguous bound for the derivative of 
\begin_inset Formula $F$
\end_inset

, too.
 From 
\begin_inset Formula 
\[
\left\Vert \frac{\partial f_{h}}{\partial y_{i}}-\frac{\partial f}{\partial y_{i}}\right\Vert _{L_{2}(\Omega')}\xrightarrow[h\to0]{}0
\]

\end_inset

it follows
\begin_inset Formula 
\begin{align*}
\left\Vert \frac{\partial}{\partial x_{i}}(f_{h}\circ y)-\frac{\partial}{\partial x_{i}}(f\circ y)\right\Vert _{L_{2}(\Omega')} & =\left\Vert \frac{\partial}{\partial x_{i}}\left((f_{h}-f)\circ y)\right)\right\Vert _{L_{2}(\Omega')}\\
 & \le\left\Vert \frac{\partial}{\partial x_{i}}\left(f_{h}-f\right)\right\Vert \left\Vert \frac{\partial}{\partial x_{i}}y\right\Vert \\
 & =\tilde{C}\left\Vert \frac{\partial f_{h}}{\partial x_{i}}-\frac{\partial f}{\partial x_{i}}\right\Vert \\
 & =\tilde{C}\left\Vert \left(\frac{\partial f}{\partial x_{i}}\right)_{h}-\frac{\partial f}{\partial x_{i}}\right\Vert \quad(\text{сменяме }\frac{\partial}{\partial x_{i}}\text{ и }\left(\cdot\right)_{h})\\
 & \xrightarrow[h\to0]{}0.
\end{align*}

\end_inset

From these estimations it follows that the function sequences
\begin_inset Formula $\left\{ F_{h=1/n}\right\} _{n=1}^{\infty}$
\end_inset

 and 
\begin_inset Formula $\left\{ \partial^{x_{i}}F_{h=1/n}\right\} _{n=1}^{\infty}$
\end_inset

 are bounded by 
\begin_inset Formula $F+L$
\end_inset

 and 
\begin_inset Formula $\partial^{x_{i}}F+L$
\end_inset

, respectively, where 
\begin_inset Formula $L$
\end_inset

 is a constant.
 Then in the definition for a generalized derivative of 
\begin_inset Formula $F_{h}=f_{h}\circ y$
\end_inset


\begin_inset Formula 
\[
\int\limits _{Q'}F_{h}\frac{\partial g}{\partial x_{i}}\text{dx}=-\int\limits _{Q'}\frac{\partial F_{h}}{\partial x_{i}}g\text{dx}\quad\text{for all }g\in C^{1}(\bar{Q})
\]

\end_inset

we can perform on both sides dominated convergence witj 
\begin_inset Formula $h\to0$
\end_inset

 and get 
\begin_inset Formula 
\[
\int\limits _{Q}\underbrace{\left(\lim_{h\to0}F_{h}\right)}_{F}\frac{\partial g}{\partial x_{i}}\text{dx}=-\int\limits _{Q}\left(\lim_{h\to0}\frac{\partial F_{h}}{\partial x_{i}}\right)g\text{dx}\quad\text{for all }g\in C^{1}(\bar{Q}),
\]

\end_inset

and now we have the first generalized derivatives of 
\begin_inset Formula $F$
\end_inset

:
\begin_inset Formula 
\[
\frac{\partial F}{\partial x_{i}}=\lim_{h\to0}\frac{\partial F_{h}}{\partial x_{i}}.
\]

\end_inset

To ensure 
\begin_inset Formula $F\in H^{1}$
\end_inset

, it remains only to verify that the derivatives are square-integrable.
 By the chain rule
\begin_inset Formula 
\[
\frac{\partial F}{\partial x_{i}}(x)=\sum_{j}\frac{\partial f}{\partial y_{j}}(y(x))\frac{\partial y_{j}}{\partial x_{i}}(x),
\]

\end_inset

we immediately get the bound 
\begin_inset Formula 
\begin{align*}
\left\Vert \frac{\partial F}{\partial x_{i}}\right\Vert  & \le\sum_{j}\left\Vert \frac{\partial f}{\partial y_{j}}\right\Vert _{L_{2}(\Omega)}\left\Vert \frac{\partial y_{j}}{\partial x_{i}}\right\Vert _{L_{2}(Q)}\\
 & \le C\left\Vert f\right\Vert _{H^{1}},
\end{align*}

\end_inset

where the constant 
\begin_inset Formula $C$
\end_inset

 depends only on 
\begin_inset Formula $y$
\end_inset

.
\end_layout

\begin_layout Proof
The derivatives of order 
\begin_inset Formula $\ge2$
\end_inset

 we get after repeated differentiation in the chain rule.
\end_layout

\begin_layout Section
Extension outside 
\begin_inset Formula $Q$
\end_inset

 with smooth boundary
\end_layout

\begin_layout Subsection
Locally
\end_layout

\begin_layout Standard
Denote by 
\begin_inset Formula $B_{r}(\xi)$
\end_inset

 the ball 
\begin_inset Formula $\left\{ x\in\mathbb{R}^{n}:\left\Vert x-\xi\right\Vert <r\right\} .$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename pics/untitled.png
	width 5cm

\end_inset


\end_layout

\begin_layout Standard
In the next lemma, we will ensure that if we have an extension of 
\begin_inset Formula $f$
\end_inset

 in a neighborhood of each point ("individually") of the boundary, then
 we can construct an extension of 
\begin_inset Formula $f$
\end_inset

 in any arbitrary extension of the considered region.
 
\end_layout

\begin_layout Lemma*
Let 
\begin_inset Formula $Q\subset\mathbb{R}^{n}$
\end_inset

 be a bounded domain, 
\begin_inset Formula $f\in H^{k}(Q)$
\end_inset

 and in each point on the boundary of 
\begin_inset Formula $Q$
\end_inset

 there is an extension of 
\begin_inset Formula $f$
\end_inset

 in some ball around that point , with 
\begin_inset Quotes gld
\end_inset

uniformly smaller
\begin_inset Quotes grd
\end_inset

 
\begin_inset Formula $H^{k}$
\end_inset

-norm than 
\begin_inset Formula $f$
\end_inset

, that is 
\begin_inset Formula 
\begin{align*}
\forall\xi\in\partial Q\quad\exists r_{\xi}>0\quad\exists & F_{\xi}\in H^{k}\left(B_{r}(\xi)\right):\\
 & F_{\xi}\big|_{Q}\equiv f\text{ and }\left\Vert F_{\xi}\right\Vert _{H^{k}(B_{r}(\xi))}\le C\left\Vert f\right\Vert _{H^{k}(Q)},
\end{align*}

\end_inset

where 
\begin_inset Formula $C$
\end_inset

 is a constant.
\end_layout

\begin_layout Lemma*
Then in every superset 
\begin_inset Formula $Q_{\rho}\coloneqq\left\{ x:\text{dist}(x,Q)<\rho\right\} $
\end_inset

 of 
\begin_inset Formula $Q$
\end_inset

 there exists an extension 
\begin_inset Formula $F\in H^{k}(Q_{\rho})$
\end_inset

 of 
\begin_inset Formula $f$
\end_inset

, vanishing outside 
\begin_inset Formula $Q_{\rho/2}$
\end_inset

 with 
\begin_inset Quotes gld
\end_inset

uniformly smaller
\begin_inset Quotes grd
\end_inset

 norm than 
\begin_inset Formula $f,$
\end_inset

 that is 
\begin_inset Formula 
\begin{align*}
\forall\rho>0\quad\exists F\in H^{k}(Q_{\rho}) & :F\big|_{Q}\equiv f,\\
 & \text{ and }F\big|_{Q_{\rho}\setminus Q_{\rho/2}}\equiv0\\
 & \text{ and }\left\Vert F\right\Vert _{H^{k}(Q_{\rho})}\le C'\left\Vert f\right\Vert _{H^{k}(Q)}
\end{align*}

\end_inset

where the constant 
\begin_inset Formula $C$
\end_inset

 does not depend on 
\begin_inset Formula $f$
\end_inset

.
\end_layout

\begin_layout Proof
With the given 
\begin_inset Formula $F_{\xi}$
